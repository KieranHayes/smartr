<h1>HOME</h1>

<h2>Latest Questions</h2>

<% Question.latest.each do |question| %>
  <div class="question">
  <div class="stats">
  
  </div>
  <div class="body">
    <h3><%= link_to(h(question.name), question_path(question)) %><span class="user"><%= question.user.login %></span></h3>
  </div>
  </div>
<% end -%>