<div class="header-with-tabs">
  <h2><%=I18n.t(:"questions.latest.header")%><%= raw "<span class='tagged'>##{params[:tag]}</span>" if params[:tag].present? %></h2>
    <span style="float: right"><%= page_entries_info @questions %></span>
  <div class="tabs">
    <%=question_submenu(:latest) %>
  </div>
  <br class="clear" />
</div>
<br class="clear" />
<% @questions.each do |question| %>
  <%= render "/questions/question", :question => question %>
<% end -%>
<br class="clear" />
<%= will_paginate @questions %>
<% content_for :sidebar, render("/questions/sidebar/default") %>
<% main_menu :questions  %>